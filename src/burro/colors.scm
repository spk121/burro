(define (color x)
  "Converts the name of a color into a 32-bit RGB hex value."
  (cond
   ((string? x)
    (let ((keyval (assoc x *color-names*)))
      (if (pair? keyval)
	  (cdr keyval)
	  ;; else, this is an unknown color name, so return magenta
	  (error "unknown color name '~a'" x))))

   (else
    ;; else, this isn't a valid integer or a string
    (error "invalid color '~s'" x))))

(define (uint32->hexstring x)
  (format #f "~8,'0x" x))
